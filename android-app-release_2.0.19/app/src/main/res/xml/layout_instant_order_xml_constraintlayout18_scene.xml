<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="500">
       <KeyFrameSet>
       </KeyFrameSet>
    </Transition>

    <ConstraintSet android:id="@+id/start">
      <Constraint
        android:layout_height="0dp"
        motion:layout_constraintStart_toStartOf="@+id/linearLayout30"
        motion:layout_constraintTop_toTopOf="@+id/linearLayout30"
        motion:layout_constraintBottom_toBottomOf="@+id/linearLayout30"
        motion:layout_constraintEnd_toEndOf="@+id/linearLayout30"
        android:layout_width="0dp"
        android:id="@+id/linearLayout34" />
      <Constraint
        android:id="@+id/linearLayout35"
        motion:layout_constraintEnd_toEndOf="@+id/linearLayout34"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        motion:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="10dp"
        motion:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="8dp"
        android:visibility="gone" />
      <Constraint
        motion:layout_constraintEnd_toStartOf="@+id/linearLayout35"
        motion:layout_constraintStart_toEndOf="@+id/linearLayout30"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        motion:layout_constraintBottom_toBottomOf="parent"
        motion:layout_constraintTop_toTopOf="parent"
        android:id="@+id/textView202"
        motion:layout_constraintHorizontal_bias="0.4" />
      <Constraint
        android:id="@+id/textView202"
        motion:layout_constraintEnd_toStartOf="@+id/linearLayout35"
        motion:layout_constraintStart_toEndOf="@+id/linearLayout30"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        motion:layout_constraintBottom_toBottomOf="parent"
        motion:layout_constraintTop_toTopOf="parent"
        android:visibility="gone" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
      <Constraint
        android:id="@+id/linearLayout35"
        motion:layout_constraintEnd_toEndOf="@+id/linearLayout34"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        motion:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="4dp"
        android:layout_marginEnd="10dp"
        motion:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="4dp"
        android:visibility="visible" />
      <Constraint
        android:id="@+id/textView202"
        motion:layout_constraintEnd_toStartOf="@+id/linearLayout35"
        motion:layout_constraintStart_toEndOf="@+id/linearLayout30"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        motion:layout_constraintBottom_toBottomOf="parent"
        motion:layout_constraintTop_toTopOf="parent"
        android:visibility="visible" />
    </ConstraintSet>
</MotionScene>