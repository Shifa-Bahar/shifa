<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data>

        <import type="android.view.View" />

        <variable
            name="quickOption"
            type="com.lifepharmacy.application.model.QuickOptionModel" />

        <variable
            name="click"
            type="com.lifepharmacy.application.ui.dashboard.adapter.HomeQuickOption" />

    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="@dimen/dimen_34dp">

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/tv_location"
            style="@style/text_10_blue_dark_open_regular"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@{quickOption.name}"
            android:textAlignment="center"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:text="Express Shipping" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>